% Some useful processing
listUrls :-
  messages(M), extractUrls(M,U), writeNice(U), nl.

extractUrls([],[]).
extractUrls([m(F,To,S,Te,U,Ty,D)|T],[(U,Ty)|Tu]) :- extractUrls(T,Tu).

writeNice([]).
writeNice([H|T]) :- write(H), nl, writeNice(T).

notSent.

