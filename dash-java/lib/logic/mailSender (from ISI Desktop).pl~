% -*- Mode: Prolog -*-

% Sends a list of initial mail messages.

:- style_check(-singleton).
:- style_check(-discontiguous).
:- style_check(-atom).

:- dynamic sent/1.

goalRequirements(doWork, [sendEmail]) :-
  notSent, !.

goalRequirements(doWork, [doNothing]).

% Define a static set of messages to send

goalRequirements(sendEmail, Goals)
  :- id(ID), 
     messages(M),
     processMessages(M,Goals).

processMessages([],[]).
% No attachment is empty set by default
processMessages([m(From,To,Subject,Text)|T],[callPerson(1,mail(From,To,Subject,Text,[],Type,Name))|GoalTail])
  :- processMessages(T,GoalTail).
% For now, send ham to someone else
processMessages([m(From,To,Subject,Text,Url,ham,DoNotReply,Name)|T],[callPerson(100,mail(From,To,Subject,Text,Url,ham,DoNotReply,Name))|GoalTail])
  :- processMessages(T,GoalTail), !.
processMessages([m(From,To,Subject,Text,Url,Type,DoNotReply,Name)|T],[callPerson(1,mail(From,To,Subject,Text,Url,Type,DoNotReply,Name))|GoalTail])
  :- processMessages(T,GoalTail).


updateBeliefs(callPerson(Person,mail(F,To,S,Te,U,Ty,D,N)),1)
  :- messages(Messages), sent(X), Y is X + 1, retract(sent(X)), assert(sent(Y)), isLastItem(m(F,To,S,Te,U,Ty,D,N),Messages), myRetract(notSent).

updateBeliefs(_,_).

isLastItem(R,[R]).
isLastItem(R,[H|T]) :- isLastItem(R,T).
  

subGoal(sendEmail).

% Reporting on state for display
state(S)
  :- sent(N), sformat(S, "sent ~w letters", [N]).

:-dynamic(notSent/0).

% Will be in bootstrap file

id(100).
goal(doWork).
goalWeight(doWork, 1) :- started.
goalWeight(waiting, 1).
sent(0).

